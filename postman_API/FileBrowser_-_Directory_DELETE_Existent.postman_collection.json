	"item": [
		{
			"name": "FileBrowser_-_Directory_POST_HELPER",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "a2a68524-fb69-41e5-b0ca-ceebe36d57fd",
						"type": "text/javascript",
						"exec": [
							"",
							"var codeObjPosted = 200;",
							"var codeObjAlreadyExist = 409;",
							"var codeObjPPathNotFound = 404;",
							"var obj = \"directory\";",
							"var responseCode = pm.response.code;",
							"var jsonData = pm.response.json();",
							"",
							"//Interpretazione del codice di errore",
							"if(responseCode === codeObjPosted)",
							"    {",
							"        pm.test(\"The \" + obj + \" has been created\", function(){",
							"            return true;",
							"        });",
							"        ",
							"        pm.test(\"Directory name are set correctly\", function(){",
							"            pm.expect(jsonData.payload.path).to.include(pm.variables.get(\"newDir_name\"));",
							"        });",
							"    }",
							"else if(responseCode === codeObjPPathNotFound)",
							"    {",
							"        pm.test(\"No parent folder with the code <parentPath> could be found\", function(){",
							"            return true;",
							"        });",
							"    }",
							"else if(responseCode === codeObjAlreadyExist)",
							"    {",
							"        if(pm.expect(jsonData.errors[0].code).to.be.eql(\"2\"))",
							"        {",
							"            pm.test(\"The \" + obj + \" already exist\", function(){",
							"                pm.expect(0).to.be.equal(1);",
							"            });",
							"        }",
							"        else",
							"        {",
							"            pm.test(\"The filename <filename> does not match with the last section of the path in the payload <path>\", function(){",
							"                return true;",
							"            });",
							"        }",
							"    }",
							"else",
							"    {",
							"        pm.test(\"Got this message \" + responseCode, function () {",
							"        pm.expect(0).to.be.equal(1); //restituisce sempre un false nel test in cui il numero errore non è fra quelli previsti",
							"        });",
							"    }",
							"",
							"",
							"//Test sul tempo di risposta",
							"pm.test(\"Response time is less than 200ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(200);",
							"});"
						]
					}
				}
			],
