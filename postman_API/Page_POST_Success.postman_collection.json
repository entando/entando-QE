							"});",
							"",
							"pm.test(\"Page code is correct\", function(){",
							"    pm.expect(pm.response.json().payload.code).to.be.eql(\"BBB\");",
							"});",
							"",
							"",
							"pm.test(\"Payload array is not empty\", function(){",
							"    var array_length = pm.response.json().payload.length;",
