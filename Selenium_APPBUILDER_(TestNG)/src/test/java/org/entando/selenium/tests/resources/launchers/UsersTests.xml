<!--src/test/resources/launchers/google.xml-->
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Users Tests" verbose="1" parallel="tests">
    <parameter name="url" value="http://appbuilder.serv.run/"/>
    <parameter name="os" value="windows"/>
    <parameter name="node" value="http://192.168.1.112:4444"/>

    <test name="Tests on Firefox" preserve-order="true" group-by-instances="true">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="DTUsersListTest"/>
        </classes>
    </test>

    <test name="Tests on Chrome" preserve-order="true" group-by-instances="true">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="DTUsersListTest"/>
        </classes>
    </test>

</suite>